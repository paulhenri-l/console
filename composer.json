{
    "name": "paulhenri-l/console",
    "description": "The PHL console",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "paulhenri-l",
            "email": "25308170+paulhenri-l@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.5",
        "laravel-zero/framework": "^7.0",
        "paulhenri-l/laravel-task-runner": "^1.0",
        "symfony/process": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "PHLConsole\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Functions/getcwd.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["phl"]
}
